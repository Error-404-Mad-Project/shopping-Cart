<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/imageViewPhoto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Instructions">

    <TextView
        android:id="@+id/textView666"
        android:layout_width="331dp"
        android:layout_height="48dp"
        android:gravity="center_horizontal"
        android:text="@string/choose_a_delivery_type"
        android:textAlignment="center"
        android:textColor="#3F51B5"
        android:textColorHint="#3F51B5"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.087"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />


    <Button
        android:id="@+id/button34"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#3F51B5"
        android:text="@string/back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RadioGroupPhoto"
        app:layout_constraintVertical_bias="0.697"
        tools:targetApi="lollipop" />

    <Button
        android:id="@+id/button38"
        android:layout_width="180dp"
        android:layout_height="54dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#3F51B5"
        android:text="@string/nextt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toEndOf="@+id/button34"
        app:layout_constraintTop_toBottomOf="@+id/RadioGroupPhoto"
        app:layout_constraintVertical_bias="0.697"
        tools:ignore="UnknownId"
        tools:targetApi="lollipop" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="135dp"
        android:layout_height="19dp"
        android:text="@string/termss"
        android:textColor="#3F51B5"
        android:textColorHint="#3F51B5"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.811" />

    <RadioGroup
        android:id="@+id/RadioGroupPhoto"
        android:layout_width="352dp"
        android:layout_height="96dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.252">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/RadioButton" />

        <RadioButton
            android:id="@+id/radioButton3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/RadioButton2" />

        <RadioButton
            android:id="@+id/radioButton4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/radio3" />
    </RadioGroup>

    <ImageView
        android:id="@+id/imageViewphoto"
        android:layout_width="288dp"
        android:layout_height="164dp"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RadioGroupPhoto"
        app:layout_constraintVertical_bias="0.27"
        app:srcCompat="@drawable/bike"
        android:contentDescription="@string/imageView2" />

</androidx.constraintlayout.widget.ConstraintLayout>